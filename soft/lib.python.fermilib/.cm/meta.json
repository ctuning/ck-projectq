{
  "auto_detect": "yes", 
  "customize": {
    "check_that_exists": "yes", 
    "env_prefix": "CK_ENV_PYTHON_PACKAGE_FERMILIB", 
    "version_module_name":      "_version",
    "version_variable_name":    "__version__",
    "limit_recursion_dir_search": {
      "linux": 5, 
      "win": 5
    }, 
    "soft_file_universal": "fermilib$#sep#$__init__.py"
  }, 
  "soft_name": "FermiLib Python library", 
  "deps": {
    "lib-projectq": {
      "local": "yes",
      "name": "ProjectQ library",
      "sort": 10,
      "tags": "lib,python-package,projectq"
    }
  },
  "tags": [
    "lib", 
    "python-package", 
    "fermilib"
  ],
  "use_customize_script_from_another_entry": {
    "module_uoa":   "soft",
    "data_uoa":     "lib.python.scipy"
  }
}
